/* miniprogram/pages/interaction/index.wxss */
.black {
  width: 100%;
  height: 24rpx;
  background-color: #f0eff6;
}

.phone_box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 80rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.phone_img {
  width: 36rpx;
  height: 32rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
  margin-right: 24rpx;
}

.phone_text {
  font-size: 28rpx;
  color: #000;
}

.phone_call {
  background-color: #c84443;
  padding: 12rpx 14rpx;
  font-size: 28rpx;
  color: #fff;
  border-radius: 10rpx;
}

.weui-textarea-counter {
  color: #b2b2b2;
  text-align: right;
}

.setImgPic {
  padding: 0 30rpx;
  box-sizing: border-box;
}

.setImgPic_tit,
.setImgPic_con {
  box-sizing: border-box;
  border-top: 1px solid #d9d9d9;
  font-size: 28rpx;
  color: #999;
}

.setImgPic_tit {
  padding: 20rpx 0;
}

.setImgPic_con {
  padding: 10px 40px 30px;
  text-align: center;
}

.setImgPic_btn {
  width: 100% !important;
  border-width: 0;
  outline: 0;
  color: #FFFFFF !important;
  background-color: #E64340 !important;
  font-weight: 400;
  font-size: 36rpx;
}

.uploader_con {
  height: 140rpx;
  margin: 20rpx 0 32rpx;
}

/* .weui-uploader__hd {
  display: none;
} */

.weui-uploader__input-box {
  width: 140rpx;
  height: 140rpx;
  border: 1px dashed rgba(195, 195, 195);
  box-sizing: border-box;
}

.weui-uploader__file {
  width: 140rpx;
  height: 140rpx;
  position: relative;
}

.close {
  width: 40rpx;
  height: 40rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100%;
  position: absolute;
  top: -10rpx;
  right: -10rpx;
  cursor: pointer;
  z-index: 999;
}