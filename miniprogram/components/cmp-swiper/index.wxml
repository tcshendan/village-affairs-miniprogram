<!--components/home/component-swiper/index.wxml-->
<swiper
  indicator-dots="{{true}}" 
  indicator-color="#d0cdd1" 
  indicator-active-color="#04be02"
  autoplay="{{autoplay}}"
  interval="{{interval}}"
  duration="{{duration}}"
>
  <block wx:for="{{swiperImages}}" wx:key="index">
    <swiper-item>
      <view class="swiper-item">
        <image src="{{item}}"></image>
      </view>
    </swiper-item>
  </block>
</swiper>
